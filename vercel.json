{
    "version": 2,
    "builds": [
      {
        "src": "autosalon-frontend/vue.config.js", 
        "use": "@vercel/vue"
      },
      {
        "src": "car-dealership-backend/server.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "/car-dealership-backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/autosalon-frontend/index.html"
      }
    ]
  }